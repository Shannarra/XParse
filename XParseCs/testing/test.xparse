<?xml version="1.0" encoding="utf-8" ?>
<content>
  <meta language="C#" codeRoot="namespace"/>
  
  <namespace name="TestNamespace">
    <dependencies>
      <ref name="System" />    
      <ref name="System.Linq" />    
      <ref name="System.Collections" />    
      <ref name="System.Xml.XmlDocument" alias="true" aliasName="Doc" />    
    </dependencies>
    <class name="MyClass" isUnsafe="false" summary="Just a test class summary" inherits="Doc, IEnumerable" protection="public">    
      <field protection="private" type="string" name="myName" value="Shannarra" isVolatile="true"/>
      <field protection="private" type="uint" name="myAge" value="69"/>
      <property protection="public" name="MyProp" type="int" additional="static" summary="This is my static property"/> 
      <property protection="public" name="MyAge" type="uint" summary="This is my accessor property" linkTo="myAge"/> 
      <property protection="public" name="MyName" type="string" summary="This is my accessor property" linkTo="myName"/> 
      <ctor/>
      <ctor protection="public" params="int value">
        <![CDATA[GreetMe();]]>
      </ctor>
      <method name="SayHi" protection="public" type="int" summary="Prints 'HI' to the console." additional="static" params=""> 
        <![CDATA[ const double pi = Math.PI; double trippled = pi*pi*pi; Console.WriteLine($"PI^3 = {trippled}"); return (int)trippled]]> 
      </method>
      <method name="GreetMe" protection="public" summary="Prints a greeting to the console." inheritsAs="virtual" params=""> 
        <![CDATA[ Console.WriteLine("hello there, I'm generated by the XParse program! PI^3 is: " + SayHi());]]> 
      </method>
      <method name="MyStatic" protection="public" additional="static" summary="Just a static void" />
    </class>
  </namespace> 
  
</content>